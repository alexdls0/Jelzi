<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimaryDark">@color/colorBackground</item>
    </style>

    <style name="darkBackground">
        <item name="android:background">@color/colorBackground</item>
    </style>

    <!--REGISTER & LOGIN-->
    <style name="SignInSignUp">
        <item name="android:textSize">@dimen/mediumSize</item>
        <item name="android:textColor">@color/colorGrey</item>
        <item name="android:layout_marginTop">@dimen/mediumMargin</item>
    </style>

    <style name="SignInSignUp.separated">
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:layout_marginTop">@dimen/none</item>
    </style>

    <style name="SignInSignUp.white">
        <item name="android:textColor">@color/colorWhite</item>
    </style>

    <style name="SignInSignUp.grey">
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorGrey</item>
        <item name="android:layout_marginTop">@dimen/none</item>
    </style>

    <style name="SignInSignUp.separated.small">
        <item name="android:textColor">@color/colorGrey</item>
        <item name="android:textSize">@dimen/halfMediumSize</item>
    </style>

    <style name="textviews">
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textSize">@dimen/textSize</item>
    </style>

    <style name="textviews.first">
        <item name="android:layout_marginTop">@dimen/bigMargin</item>
    </style>

    <style name="textviews.standard">
        <item name="android:layout_marginTop">@dimen/mediumMargin</item>
    </style>

    <style name="edittexts">
        <item name="android:textColor">@color/colorGrey</item>
        <item name="android:textColorHint">@color/colorGrey</item>
        <item name="android:backgroundTint">@color/colorGrey</item>
        <item name="android:textSize">@dimen/smallSize</item>
        <item name="android:layout_marginTop">@dimen/topMarginEdittexts</item>
    </style>

    <style name="buttons">
        <item name="android:background">@drawable/btn_bg</item>
        <item name="android:paddingStart">@dimen/mediumMargin</item>
        <item name="android:paddingEnd">@dimen/mediumMargin</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:layout_marginTop">@dimen/mediumMargin</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="alreadyMember">
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textSize">@dimen/smallSize</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
    </style>

    <style name="forgotpass">
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textSize">@dimen/smallSize</item>
        <item name="android:layout_marginTop">@dimen/bigMargin</item>
    </style>

    <style name="jelziimagotipo">
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
        <item name="android:layout_height">@dimen/jelziimagotipoheight</item>
        <item name="android:layout_width">@dimen/jelziimagotipowidth</item>
    </style>

    <style name="SearchViewStyle" parent="Widget.AppCompat.SearchView">
        <item name="android:textColor">@color/white</item>
        <item name="android:editTextColor">@color/white</item>
        <item name="android:textColorHint">@color/colorGrey</item>
    </style>

    <style name="textUser">
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/textSize</item>
    </style>

    <style name="textMacros">
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/textSize</item>
    </style>

    <style name="textMacrosNumbers">
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textSize">@dimen/textSize</item>
    </style>

    <style name="cardFood">
        <item name="android:textSize">@dimen/smallSize</item>
    </style>
    <style name="FirstCalcTittles">
        <item name="android:textSize">@dimen/bigSize</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colorWhite</item>
    </style>
    <style name="FirstCalcTextSelects">
        <item name="android:textSize">@dimen/mediumSize</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colorWhite</item>
    </style>

    <style name="ThemeOverlay.NumberPicker" parent="AppTheme" >
        <item name="colorControlNormal">@color/colorGreen</item>
        <item name="colorControlActivated">@color/colorGreen</item>
        <item name="colorControlHighlight">@color/colorGreen</item>
        <item name="android:textColorPrimary">@color/colorWhite</item>
    </style>

    <style name="loadingtextview">
        <item name="android:textSize">@dimen/smallSize</item>
        <item name="android:layout_marginTop">@dimen/halfMediumSize</item>
    </style>
    <style name="darkBackgroundCards">
        <item name="android:background">@drawable/cards_background</item>
    </style>
    <style name="profileOptions">
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textSize">@dimen/halfMediumSize</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
        <item name="android:layout_marginBottom">@dimen/smallMargin</item>
    </style>

    <style name="profileData">
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/halfMediumSize</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
    </style>

    <style name="profileLogout">
        <item name="android:textSize">@dimen/halfMediumSize</item>
        <item name="android:background">@drawable/btn_bg_logout</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:layout_marginEnd">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="profileRemoveTracing">
        <item name="android:textSize">@dimen/halfMediumSize</item>
        <item name="android:background">@drawable/btn_bg_red</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:layout_marginEnd">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorRed</item>
        <item name="android:layout_marginTop">@dimen/mediumMargin</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="optionsProfile">
        <item name="android:background">@drawable/btn_bg_background_color</item>
        <item name="android:layout_marginTop">@dimen/smallSize</item>
        <item name="android:layout_marginBottom">@dimen/smallSize</item>
    </style>
    <style name="searchView">
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textColorHint">@color/colorWhite</item>
    </style>
    <style name="optionsProfileDialog">
        <item name="android:background">@color/colorBackground</item>
        <item name="android:padding">@dimen/smallMargin</item>
    </style>

    <style name="optionsProfileDialogUsernameEt">
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:layout_marginEnd">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:background">@drawable/rect_shape</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingTop">@dimen/smallMargin</item>
        <item name="android:paddingBottom">@dimen/smallMargin</item>
        <item name="android:paddingStart">@dimen/mediumMargin</item>
        <item name="android:paddingEnd">@dimen/negativemediumMargin</item>
    </style>

    <style name="optionsProfileDialogUsernameBt">
        <item name="android:layout_marginTop">@dimen/mediumMargin</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:layout_marginEnd">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:background">@drawable/btn_bg_logout</item>
    </style>

    <style name="optionsProfileDialogBtChange">
        <item name="android:layout_marginTop">@dimen/mediumMargin</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:layout_marginEnd">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorGreen</item>
        <item name="android:background">@drawable/btn_bg_change</item>
    </style>

    <style name="profileRemoveTracingText">
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
        <item name="android:textSize">@dimen/halfMediumSize</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:layout_marginEnd">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorWhite</item>
    </style>

    <style name="dialogErrorTitle">
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
        <item name="android:textSize">@dimen/mediumSize</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:layout_marginEnd">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="dialogErrorText">
        <item name="android:layout_marginTop">@dimen/mediumMargin</item>
        <item name="android:textSize">@dimen/halfMediumSize</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:layout_marginEnd">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorWhite</item>
    </style>

    <style name="profileLogout.continueButton">
        <item name="android:layout_marginTop">@dimen/mediumMargin</item>
    </style>

    <style name="forgotPassDialog">
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
        <item name="android:textSize">@dimen/mediumSize</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:layout_marginEnd">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorWhite</item>
    </style>

    <style name="forgotPassDialogMessage">
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
        <item name="android:textSize">@dimen/halfMediumSize</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:layout_marginEnd">@dimen/smallMargin</item>
        <item name="android:textColor">@color/colorWhite</item>
    </style>

    <style name="forgotPassEt">
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
        <item name="android:layout_marginStart">@dimen/smallMargin</item>
        <item name="android:layout_marginEnd">@dimen/smallMargin</item>
        <item name="android:textSize">@dimen/halfMediumSize</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:background">@drawable/rect_shape</item>
        <item name="android:paddingTop">@dimen/smallMargin</item>
        <item name="android:paddingBottom">@dimen/smallMargin</item>
        <item name="android:paddingStart">@dimen/smallMargin</item>
    </style>

    <style name="dialogGenderMarginTop">
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
    </style>

    <style name="FirstCalcTextSelects.dialogGenderMarginTop">
        <item name="android:layout_marginTop">@dimen/smallMargin</item>
    </style>

    <style name="FirstCalcTextSelects.dialogGenderMarginBottom">
        <item name="android:layout_marginBottom">@dimen/smallMargin</item>
    </style>

    <style name="FirstCalcTextSelects.dialogObjectivePaddingBottom">
        <item name="android:paddingBottom">@dimen/smallMargin</item>
    </style>
    <style name="edittextServing">
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textSize">@dimen/textSize</item>
    </style>
</resources>